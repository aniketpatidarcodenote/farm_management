<%= form_with model: [ @farmer, @farmer.vegetables.build ] do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name%>
  </div>
  <div>
    <%= form.label :price %><br>
    <%= form.number_field :price %>
  </div>
  <div>
    <%= form.label :stock %><br>
    <%= form.number_field :stock %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>